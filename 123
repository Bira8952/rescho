local whitelistedUserIds = {
    3342385890, -- NRWOtuziki
    7307129283, -- DATS
    7512855479, -- Symen
    4,
}
local function isWhitelisted(userId)
    for _, id in ipairs(whitelistedUserIds) do
        if id == userId then
            return true
        end
    end
    return false
end
local player = game.Players.LocalPlayer
if not isWhitelisted(player.UserId) then
    player:Kick("you have not been verified by the script creators sorry ):")
else
    print("you have been successfully verified now you can play (:")
end


